
window.onload = function () {
    // title
    var t = document.getElementsByTagName('title');
    if (!!t) {
        t = t[0];
        t.innerHTML = t.innerHTML.replace(/秒/g, '**');
		 t.innerHTML = t.innerHTML.replace(/刷/g, '**');
		
		 t.innerHTML = t.innerHTML.replace(/自动点赞/g, '**');
		 
    }
 
}

